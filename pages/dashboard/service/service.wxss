
page{
  height: 100%;
}
.main{
  /* display:flex; */
  position: relative;
  /* flex-direction:column; */
  height: 100%;
  width: 100%;
  background-color:rgb(233, 234, 236);
  text-align: center;
}

.serviceButton{
  /* position: absolute; */
  width: 50%;
  top: 55%;
  background-color: #215089;
  color: #fff;
  border-radius: 23px;
}

.textPhone{
  position: absolute;
  color: #215089;
  /* bottom: 80px; */
  top: 83%;
  font-size: 16px;
  left: 50%;
  transform: translateX(-50%);
}

.iconView{
  /* position: absolute; */
  border: 2px solid #215089;
  width: 120px;
  height: 120px;
  left: 50%;
  transform: translateX(-50%);
  position: relative;
  top:20%;
  border-radius: 10px;
}


